{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/*/*.ts",
    "src/environments/environment.prod.ts",
    "src/main.ts",
    "src/polyfills.ts",
    "src/test.ts",
  ],
  "include": [
    "src/*/*.d.ts"
  ],
  "exclude": [
    "src/*/*.spec.ts"
  ]
}
